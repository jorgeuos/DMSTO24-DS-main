def filter_odd(numbers: list) -> list:
    return [num for num in numbers if num % 2 == 0]

print(filter_odd([1, 2, 3, 4, 5, 6]))    # Output: [2, 4, 6]
print(filter_odd([7, 9, 11]))            # Output: []
print(filter_odd([10, 12, 15, 18]))      # Output: [10, 12, 18]
print(filter_odd ([]))                    # Output: []
